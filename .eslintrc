{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
    },
    "rules": {
        "semi":"off",
        "comma-dangle":"off",
        "arrow-parens":"off",
        "require-jsdoc":"off",
        "linebreak-style":"off",
        "quotes":"off",
        "indent":"off",
        "object-curly-spacing":"off",
    },
    "extends": ["eslint:recommended", "google"]
}